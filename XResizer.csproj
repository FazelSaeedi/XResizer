<Project Sdk="Microsoft.NET.Sdk">
  <Sdk Name="Microsoft.Build.CentralPackageVersions" Version="2.1.3" />

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageId>XResizer</PackageId>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <Version>1.0.2</Version>
    <Authors>FazelSaeedi</Authors>
    <Company>FazelSaeedi</Company>
    <Product>FazelSaeedi.XResizer</Product>
    <Description>This package provides methods to resize image in order to reducing its size .</Description>
    <PackageTags>XResizer,FazelSaeedi</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="SixLabors.ImageSharp" Version="2.1.1" />
  </ItemGroup>


</Project>
